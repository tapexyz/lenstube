{
  "name": "@tape.xyz/api",
  "description": "API for tape.xyz",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "db:migrate": "prisma migrate dev --schema ./src/db/schema.prisma",
    "prisma:format": "prisma format --schema ./src/db/schema.prisma",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "typecheck": "tsc --pretty",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,css}\""
  },
  "dependencies": {
    "@aws-sdk/client-sts": "3.624.0",
    "@hono/node-server": "^1.11.2",
    "@hono/zod-validator": "^0.2.2",
    "@lens-protocol/metadata": "^1.2.0",
    "base64url": "^3.0.1",
    "hono": "^4.5.3",
    "linkedom": "^0.18.4",
    "prisma": "^5.17.0",
    "ua-parser-js": "2.0.0-beta.3",
    "viem": "^2.18.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tape.xyz/abis": "workspace:*",
    "@tape.xyz/config": "workspace:*",
    "@tape.xyz/constants": "workspace:*",
    "@tape.xyz/generic": "workspace:*",
    "@tape.xyz/server": "workspace:*",
    "@types/node": "^22.1.0",
    "@types/ua-parser-js": "^0.7.39",
    "tsx": "^4.16.5",
    "typescript": "5.5.4"
  }
}
