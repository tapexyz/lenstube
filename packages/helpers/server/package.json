{
  "name": "@tape.xyz/server",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx",
    "typecheck": "tsc --pretty",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,css}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,css}\"",
    "psql:migrate": "prisma migrate dev --schema ./db/psql/schema.prisma",
    "prisma:generate": "prisma generate --schema ./db/psql/schema.prisma",
    "postinstall": "pnpm prisma:generate"
  },
  "dependencies": {
    "@clickhouse/client": "^1.4.1",
    "@prisma/client": "^5.18.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@tape.xyz/config": "workspace:*",
    "@types/node": "^22.2.0",
    "prisma": "^5.18.0",
    "typescript": "5.5.4"
  }
}
