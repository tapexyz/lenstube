{
  "name": "@tape.xyz/lens",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "main": "./generated.ts",
  "private": true,
  "scripts": {
    "codegen": "graphql-codegen",
    "codegenv3": "graphql-codegen --config codegenv3.ts",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "typecheck": "tsc --pretty",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,css,graphql}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,css,graphql}\""
  },
  "dependencies": {
    "@apollo/client": "3.11.8",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@lens-protocol/metadata": "^1.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/fragment-matcher": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tape.xyz/config": "workspace:*",
    "@tape.xyz/constants": "workspace:*",
    "@types/node": "22.5.4",
    "typescript": "5.5.4"
  }
}
